import java.util.*;

public class Main{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        int[] nums = new int[n-1];
        for(int i=0;i<n-1;i++){
            nums[i] = scan.nextInt();
        }
        int total = n*(n+1)/2;
        int sum=0;
        for(int i=0;i<n-1;i++){
            sum +=nums[i];
        }System.out.println(total-sum);
        }
    }
